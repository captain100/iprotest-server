            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-sm-4">
                    <h2>Project list</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li class="active">
                            <strong>Project list</strong>
                        </li>
                    </ol>
                </div>
            </div>

            <div class="wrapper wrapper-content">
                <!-- 项目列表 -->
                <div class="ibox">
                    <div class="ibox-title">
                        <h5>All projects assigned to this account</h5>
                        <div class="ibox-tools">
                            <a href="/createProject" class="btn btn-primary btn-xs">Create new project</a>
                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="row m-b-sm m-t-sm" style="display: none">
                            <div class="col-md-1">
                                <button type="button" id="loading-example-btn" class="btn btn-white btn-sm" ><i class="fa fa-refresh"></i> Refresh</button>
                            </div>
                            <div class="col-md-11">
                                <div class="input-group"><input type="text" placeholder="Search" class="input-sm form-control"> <span class="input-group-btn">
                                    <button type="button" class="btn btn-sm btn-primary"> Go!</button> </span></div>
                            </div>
                        </div>

                        <div class="project-list">
                            <table class="table table-hover">
                                <tbody>
                                    <% for(var i = 0;i < list.length;i++){ %>
                                        <tr>
                                            <td class="project-status">
                                                <a href="/getProjectCompleted?projectNo=<%=list[i].projectNo%>">
                                                    <%=list[i].projectNo%>
                                                </a>
                                            </td>

                                            <td class="project-title">
                                                <a href="/getProjectCompleted?projectNo=<%=list[i].projectNo%>">
                                                    <%= list[i].projectName%>
                                                </a>
                                            </td>
                                            <td class="project-completion">
                                                <span><%= new Date(list[i].updateTime) %></span>
                                            </td>

                                            <td class="project-actions">
                                                <a href="/getProjectCompleted?projectNo=<%=list[i].projectNo%>" class="btn btn-white btn-sm"><i class="fa fa-folder"></i> View </a>
                                                <a href="/getProjectNo?projectNo=<%=list[i].projectNo%>" class="btn btn-white btn-sm"><i class="fa fa-pencil"></i> Edit </a>
                                            </td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>


            </div>
            <div id="right-wrapper"></div>
        </div>



